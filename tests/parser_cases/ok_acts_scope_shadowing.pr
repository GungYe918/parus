field Vec2 {
    x: i32;
    y: i32;
};

acts for Vec2 {
    def add(self v: Vec2, rhs: i32) -> i32 {
        return v.x + rhs;
    }
};

acts Alt for Vec2 {
    def add(self v: Vec2, rhs: i32) -> i32 {
        return v.y + rhs;
    }
};

def main() -> i32 {
    let v: Vec2 = Vec2 { x: 10i32, y: 40i32 };
    let a: i32 = v.add(1i32);
    {
        use Vec2 with acts(Alt);
        let b: i32 = v.add(1i32);
    }
    use Vec2 with acts(default);
    let c: i32 = v.add(1i32);
    return a + c;
}
