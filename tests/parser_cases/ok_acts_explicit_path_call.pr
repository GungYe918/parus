field Vec2 {
    x: i32;
    y: i32;
};

acts A for Vec2 {
    def add(a: i32, b: i32) -> i32 {
        return a + b;
    }

    def sum_xy(self v: &Vec2) -> i32 {
        return v.x + v.y;
    }
};

def main() -> i32 {
    let v: Vec2 = Vec2 {x: 40, y: 2};
    let a: i32 = Vec2::acts(A)::add(1, 2);
    let b: i32 = Vec2::acts(A)::sum_xy(&v);
    return a + b;
}
