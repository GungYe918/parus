field Vec2 {
    x: i32;
    y: i32;
};

acts MathOps for Vec2 {
    def add(self v: Vec2, rhs: i32) -> i32 {
        return v.x + rhs;
    }
};

// legacy syntax must be rejected:
use acts MathOps for Vec2;

def main() -> i32 {
    let v: Vec2 = Vec2 { x: 1i32, y: 2i32 };
    return v.add(3i32);
}
