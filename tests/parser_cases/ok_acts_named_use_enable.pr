field Vec2 {
    x: i32;
    y: i32;
};

use acts MathOps for Vec2;

acts MathOps for Vec2 {
    def add(self v: Vec2, rhs: i32) -> i32 {
        return v.x + rhs;
    }

    operator(+)(self a: Vec2, rhs: i32) -> i32 {
        return a.x + rhs;
    }
};

def main() -> i32 {
    let v: Vec2 = Vec2 {x: 5, y: 7};
    let a: i32 = v.add(3);
    let b: i32 = v + 4;
    return a + b;
}
