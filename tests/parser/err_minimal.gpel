fn mul({ a: i32, b: i32 }) -> i32 {
    return a * b;
}

fn add(a: i32, {b: i32 = 5, c: i32 = 6}) -> i32 {
    return a + b + c; 
}

fn main() -> i32 {
    mul(a: 4, b: 5); // 이건 정상 통과 -> but 오류 발생
    
    mul(a: 6, 6);   // 라벨 호출 + 일반 호출 양립 불가(한 named-group안임) -> 여기서는 에러로 잡힘
    mul(5, 5);  // 이건 정상 통과여야 함 -> 오류로 잡힘
    //mul(a: 6);  // 인자 하나가 비었고, 기본값도 선언되지 않았으니 오류 -> but 오류 메세지가 이상함(인자 0개 기대라고 표시)

    add(5); // 이건 정상 통과여야 함. -> no 오류
    add(5, {c: 5, b: 6}); // 이것도 정상 통과 -> No 오류
    
    return 42;
}