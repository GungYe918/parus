def mul({ a: i32, b: i32 }) -> i32 {
    return a * b;
}

def add(a: i32, {b: i32 = 5, c: i32 = 6}) -> i32 {
    return a + b + c; 
}

def main() -> i32 {
    mul(a: 4, b: 5); // 이건 정상 통과
    
    // mul(a: 6, 6);   // 라벨 호출 + 일반 호출 양립 불가(한 named-group안임) -> 여기서는 에러로 잡힘
    // mul(5, 5);  // 이건 오류 (named-only 함수)

    add(5);
    add(5, c: 5, b: 6);
    // add(2, 42, 6); // 이건 오류 (named-group tail은 라벨 필요)
    
    return 42;
}
