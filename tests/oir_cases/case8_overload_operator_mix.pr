// OIR->LLVM 케이스: 함수 오버로딩 + field operator 오버로딩 혼합
field I32Box {
    v: i32;
};

acts for I32Box {
    operator(+)(self a: I32Box, rhs: I32Box) -> I32Box {
        return I32Box { v: a.v + rhs.v };
    }
};

def mix(a: i32, b: i32) -> i32 {
    return a + b;
}

def mix(a: i64, b: i64) -> i64 {
    return a + b;
}

def main() -> i32 {
    let p: i32 = mix(a: 7i32, b: 8i32);
    let q: i64 = mix(a: 9i64, b: 10i64);
    let r: I32Box = I32Box { v: p } + I32Box { v: 1i32 };
    return r.v;
}
