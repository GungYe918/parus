// OIR->LLVM 케이스: named acts set 활성화(use T with acts) 후 메서드/연산자 해소
field Vec2 {
    x: i32;
    y: i32;
};

acts MathOps for Vec2 {
    def add(self v: Vec2, rhs: i32) -> i32 {
        return v.x + rhs;
    }

    operator(+)(self a: Vec2, rhs: i32) -> i32 {
        return a.x + rhs;
    }
};

use Vec2 with acts(MathOps);

def main() -> i32 {
    let v: Vec2 = Vec2 {x: 11, y: 22};
    let a: i32 = v.add(3);
    let b: i32 = v + 4;
    return a + b;
}
