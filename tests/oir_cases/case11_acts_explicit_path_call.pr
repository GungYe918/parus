struct Vec2 {
    x: i32;
    y: i32;
};

acts A for Vec2 {
    def add(self, rhs: i32) -> i32 {
        return self.x + rhs;
    }

    def sum_xy(self) -> i32 {
        return self.x + self.y;
    }
};

def main() -> i32 {
    let v: Vec2 = Vec2 {x: 20, y: 22};
    let a: i32 = Vec2::acts(A)::add(&v, 11);
    let b: i32 = Vec2::acts(A)::sum_xy(&v);
    return a + b;
}
