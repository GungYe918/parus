// OIR->LLVM 케이스: field 기반 acts for operator(+) 오버로딩
field I32Box {
    v: i32;
};

acts for I32Box {
    operator(+)(self a: I32Box, rhs: I32Box) -> I32Box {
        return I32Box { v: a.v + rhs.v };
    }
};

def main() -> i32 {
    let a: I32Box = I32Box { v: 1i32 };
    let b: I32Box = I32Box { v: 2i32 };
    let c: I32Box = a + b;
    return c.v;
}
