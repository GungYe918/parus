plan a_bundle = bundle & {
  name = "a";
  kind = "lib";
  modules = [
    module & {
      head = "a";
      sources = ["src/a.pr"];
      imports = ["b"];
    },
  ];
  deps = ["b"];
};

plan b_bundle = bundle & {
  name = "b";
  kind = "lib";
  modules = [
    module & {
      head = "b";
      sources = ["src/b.pr"];
      imports = ["a"];
    },
  ];
  deps = ["a"];
};

plan master = master & {
  project = {
    name: "cycle",
    version: "0.1.0",
  };
  bundles = [a_bundle, b_bundle];
  tasks = [];
  codegens = [];
};
